package BuscaProfundidade;

public class Aresta {

    private int id;
    private Vertice A = null;
    private Vertice B = null;

    public Aresta(Vertice a, Vertice b) {
        this.A = a;
        this.B = b;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public Vertice getA() {
        return A;
    }

    public void setA(Vertice A) {
        this.A = A;
    }

    public Vertice getB() {
        return B;
    }

    public void setB(Vertice B) {
        this.B = B;
    }
    
}
